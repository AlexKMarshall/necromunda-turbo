    "eslint": "8.24.0",
    "eslint-config-custom": "workspace:*",
    "jest": "29.1.2",
    "jest-presets": "workspace:*",
    "react": "18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "6.2.3",
    "typescript": "4.8.4"
